{
    "hosting": {
      "public": "dist",
      "rewrites": [{
        "source": "**",
        "destination": "/index.html"
      },{
        "source": "**",
        "function": "app"
      }],
      "ignore": [
        "firebase.json",
        "**/.*",
        "**/node_modules/**"
      ]
    },
    "functions": {
      "source": "functions"
    }
  }
